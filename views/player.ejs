                    setTimeout(() => player[0].pause(), 100)
                }

                function notifyMovieEnded() {
                    sendFramesToServer();
                }

                function sendFramesToServer() {
                    const scenes = player[0].getSceneInfoArray();
                    const frames = player[0].getPhotoArray();
                    for (const scene of scenes) frames.splice(scene.startFrom, 1);
                    $.ajax({
                        url: "/api/videoExport/completed",
                        method: "POST",
                        data: JSON.stringify({
                            frames,
                            id: '<%= params.flashvars.movieId %>'
                        }),
                        dataType: "json",
                        contentType: "application/json"
                    }).done((d) => { // notify the h5 player that the video is ready.
                        if (d) {
                            player.hide();
                            videoLoadingComplete = true;
                            $("#h5-player").html(`<source src="${d.videoUrl}" type="video/mp4">`);
                            $("#video-loading").hide();
                            $(".video-loading-message").text("");
                            $("#player-control").show();
                            document.getElementById('video-download').download = flashExporterVars.movieTitle.split("+").join(" ");
                            document.getElementById('video-download').href = d.videoUrl;
                            $("#export").show();
                        } else $(".video-loading-message").text("could not fetch your video. please try again later.")
                    });
                }
                function switchPlayers(type) {
                    switch (type) {
                        case "flash": {
                            switchToFlashPlayer();
                            break;
                        } case "h5": {
                            if ($(".video-player-viewport").hasClass("using-flash")) {
                                $(".video-player-viewport").removeClass("using-flash");
                                $(".player-container > .notification").addClass("hidden").filter(".using-h5").removeClass("hidden");
                                $(".shortcut-instruction").removeClass("hidden");
                                $("#flash-player").addClass("hidden");
                                if (!videoLoadingComplete) {
                                    $("#video-loading").show();
                                    $(".video-loading-message").text('This video is currently loading right now. please wait for a few minutes.');
                                    $("#player-control").hide();
                                }
                            }
                            break;
                        }
                    }
                }
                if (!check4FlashPlayer()) { // flash player is required in order to export a movie. if it's non existant, just check for an already exported movie.
                    $.post("/api/check4ExportedMovieExistance", {id: '<%= params.flashvars.movieId %>'}).done(d => {
                        player.addClass("hidden");
                        const json = JSON.parse(d);
                        if (json.exists) {
                            videoLoadingComplete = true;
                            $("#h5-player").html(`<source src="/movies/<%= params.flashvars.movieId %>.mp4" type="video/mp4">`);
                            $("#video-loading").hide();
                            $(".video-loading-message").text("");
                            $("#player-control").show();
                            document.getElementById('video-download').download = flashExporterVars.movieTitle.split("+").join(" ");
                            document.getElementById('video-download').href = '/movies/<%= params.flashvars.movieId %>.mp4';
                            $("#export").show();
                        } else $(".video-loading-message").html("You don't have adobe flash player and an exported version of this movie does not exist. <br>please ask someone with flash player to export this movie for you.");
                    })
                }
            </script>
