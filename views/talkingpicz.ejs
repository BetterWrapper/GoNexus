    <script>
        jQuery.flash.expressInstall = "/static/libs/expressInstall.swf";
        (function($) {
            $(document).ready(function() {
                $(document).trigger('GoLite.stateChange', ['']);
                var next = 'step1', prev = '';
                $('#nav_links .next').click(function(e) {
                    e.preventDefault();
                    next && stepTo('#' + next);
                });
                $('#nav_links .prev').click(function(e) {
                    e.preventDefault();
                    prev && stepTo('#' + prev);
                });
                $('.section').waypoint({offset: '50%'});
                $('body').delegate('.section', 'waypoint.reached', function(e, direction) {
                    var $this = $(this);
                    var id = $this.attr('id');
                    if (direction == 'up') {
                        if (id == 'step1') {
                            next = 'step1';
                            prev = '';
                            id = 'intro';
                        } else if (id == 'step3') {
                            next = 'step3';
                            prev = 'intro';
                            id = 'step1';
                        } else if (id == 'step4') {
                            next = 'step4';
                            prev = 'step1';
                            id = 'step3';
                        }
                    } else {
                        if (id == 'step1') {
                            next = 'step3';
                            prev = 'intro';
                        } else if (id == 'step3') {
                            next = 'step4';
                            prev = 'step1';
                        } else if (id == 'step4') {
                            next = '';
                            prev = 'step3';
                        }
                    }
                    $('#nav_links').find('li').removeClass('on').end().find('.' + id).addClass('on');
                });
                jQuery('#movie_title, #movie_description').placeholder();
            });
            $(window).load(function() {$('#nav_links').fadeIn();});
        })(jQuery);
    </script>
    
