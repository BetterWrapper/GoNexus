<script>
(function($) {
var processing = false;
function validateLogin() {
    var email = $('#login-email'), email_val = $.trim(email.val());

    formErrorMessage('');

    if (!email_val) {
        $('#login-email').focus();
        formErrorMessage('Please enter your email address');
        return false;
    }

    return true;
}

function formErrorMessage(message) {
    $('#login-form-message').text(message);
}

function submitLogin() {
    if (!validateLogin()) return;
    if (processing) return;

    processing = true;

    var buttonText = $('#btn-login').text();
    $('#btn-login').text('Generating A Password Reset Link...');
    auth.sendPasswordResetEmail($('#login-email').val()).then(() => {
        $("#login-container").hide();
        $("#success-container").show();
    }).catch(e => {
        processing = false;
        $("#btn-login").text(buttonText);
        formErrorMessage(e.message);
    });
}

$('#login-form').submit(function() {
    submitLogin();
    return false;
});

if (!$('#login-email').val().length) $('#login-email').focus();


})(jQuery);
</script>
