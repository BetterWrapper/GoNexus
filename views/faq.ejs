        <script type="text/javascript" src="/js/waypoint.js"></script>
        <script>
            jQuery("#gonexussignup").text(`${window.location.origin}/public_signup`);
            for (const element of document.getElementsByClassName("category_title")) {
                new Waypoint({
                    element,
                    handler() {
                        for (const elem of document.getElementsByClassName("stepLink")) {
                            jQuery(`.${jQuery(elem.innerHTML).attr("class").split(" active")[0]}`).removeClass("active");
                        }
                        jQuery(`.${element.id}`).addClass("active");
                    }
                });
            }
            ((params) => {
                if (params.get("target") && (params.get("action") || params.get("open"))) {
                    switch (params.get("action")) {
                        case "openModal": {
                            jQuery(`#${params.get("target")}`).modal({
                                keyboard: false,
                                backdrop: "static"
                            });
                            break;
                        } case "getExplination": {
                            jQuery(`#${params.get("open")}`).collapse();
                            break;
                        }
                    }
                }
            })(new URLSearchParams(window.location.search));
        </script>
